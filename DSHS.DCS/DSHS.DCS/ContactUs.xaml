	 <ScrollView>
<AbsoluteLayout>
			<TableView HasUnevenRows="True"  AbsoluteLayout.LayoutBounds="0, 0, 0.5, 0.5"
                AbsoluteLayout.LayoutFlags="All">
				 <TableView.Root>
						<TableSection Title="A">
							 <!--<EntryCell Label="EntryCell"></EntryCell>-->
							 <!--<TextCell Text="{Binding PhysicalAddress}" Detail="{Binding PhysicalAddressCityStateZip}"></TextCell>-->
								 
							 <ViewCell Height="10">
									<ViewCell.View>
										 <StackLayout>
