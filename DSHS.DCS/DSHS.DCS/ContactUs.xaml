													 <Label Text=">" TextColor="Black" FontAttributes="Bold" HorizontalOptions="EndAndExpand"></Label>
												</StackLayout>
												<!--<BoxView x:Name="boxView" Color="Blue" ></BoxView>-->
												
										
										 </StackLayout>
									
									
									</ViewCell.View>
							
							 </ViewCell>
						
				 <ViewCell>
									<ViewCell.View>
										 <StackLayout>
												<!--<BoxView Color="Red"></BoxView>-->
												<StackLayout Orientation="Horizontal"  VerticalOptions="CenterAndExpand" >
